{
  "compilerOptions": {
    "outDir": "dist",
    "esModuleInterop": true,
    "declaration": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "skipLibCheck": true,
    "target": "ES2022",
    "lib": [
      "ES2022",
    ],
    "inlineSourceMap": true,
    "module": "Node16",
    "moduleResolution": "Node16",
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "src/**/*.test.ts"
  ]
}
